<p id="notice"><%= notice %></p>

<h1>Listing Graduation Theses</h1>

<table>
  <thead>
    <tr>
      <th>Past assign</th>
      <th>Name</th>
      <th>View hash</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @graduation_theses.each do |graduation_thesis| %>
      <tr>
        <td><%= graduation_thesis.past_assign %></td>
        <td><%= graduation_thesis.name %></td>
        <td><%= graduation_thesis.view_hash %></td>
        <td><%= link_to 'Show', graduation_thesis %></td>
        <td><%= link_to 'Edit', edit_graduation_thesis_path(graduation_thesis) %></td>
        <td><%= link_to 'Destroy', graduation_thesis, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

